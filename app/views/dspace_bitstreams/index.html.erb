<p id="notice"><%= notice %></p>

<h1>Dspace Bitstreams</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @dspace_bitstreams.each do |dspace_bitstream| %>
      <tr>
        <td><%= link_to dspace_bitstream.name, dspace_bitstream_path(dspace_bitstream.id) %></td>
        <td><%= link_to 'Edit', edit_dspace_bitstream_path(dspace_bitstream.id) %></td>
        <td><%= link_to 'Destroy', dspace_bitstream_path(dspace_bitstream.id), method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Dspace Bitstream', new_dspace_bitstream_path %>
