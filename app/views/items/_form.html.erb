<%= simple_form_for(@item) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name, :label => "Name" %>
  </div>

  <div class="form-actions">
    <%= f.button :submit, "Salvar", class: "btn btn-primary" %>
    <%= link_to "Voltar", :back,  class: "btn btn-warning" %>
  </div>
<% end %>
